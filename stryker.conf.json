{
  "testRunner": "jest",
  "coverageAnalysis": "off",
  "ignorePatterns": ["**/node_modules/**", "**/dist/**"],
  "mutate": ["backend/**/*.js", "!**/*.test.js"],
  "jest": {
    "projectType": "custom",
    "configFile": "jest.config.js"
  },
  "thresholds": {
    "high": 80,
    "low": 70,
    "break": 70
  }
}
