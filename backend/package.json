{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon --experimental-specifier-resolution=node server/index.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "lint": "eslint . --ext .js,.mjs",
    "test": "vitest run --no-threads",
    "test:watch": "vitest",
    "test:unit": "vitest run __tests__/unit",
    "test:integration": "vitest run __tests__/integration",
    "test:e2e": "vitest run __tests__/e2e",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@opentelemetry/exporter-prometheus": "^0.32.0",
    "@opentelemetry/sdk-node": "^0.32.0",
    "@zilliz/milvus2-sdk-node": "^2.3.0",
    "chromadb": "^3.0.10",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.7.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "c8": "^8.0.1",
    "jest-environment-node": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "supertest": "^6.3.3",
    "vitest": "^0.34.4"
  }
}
