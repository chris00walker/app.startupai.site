# StartupAI Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
DATABASE_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:6543/postgres?pgbouncer=true

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_MARKETING_URL=http://localhost:3001

# Analytics & Product Analytics
NEXT_PUBLIC_POSTHOG_KEY=phc_your_key_here
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# Development
NODE_ENV=development
NEXT_PUBLIC_ENABLE_CLICK_TO_COMPONENT=true

# Agentuity Agent Configuration
# Production webhook URL (replace with your deployed agent ID)
AGENTUITY_AGENT_URL=https://agentuity.ai/webhook/YOUR_AGENT_ID
# For local development with Agentuity CLI:
# AGENTUITY_AGENT_URL=http://localhost:3500

# Legacy CrewAI Configuration (being replaced by Agentuity)
# CREW_ANALYZE_URL=http://localhost:8888/.netlify/functions/crew-analyze
