{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon --experimental-specifier-resolution=node server/index.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "lint": "eslint . --ext .js,.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run __tests__/unit",
    "test:integration": "vitest run __tests__/integration",
    "test:e2e": "vitest run __tests__/e2e",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@opentelemetry/exporter-prometheus": "^0.54.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@zilliz/milvus2-sdk-node": "^2.4.8",
    "bcryptjs": "^3.0.2",
    "chromadb": "^3.0.10",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^8.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.4",
    "openai": "^4.73.1",
    "puppeteer": "^24.15.0",
    "sharp": "^0.34.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "c8": "^10.1.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mongodb-memory-server": "^10.1.1",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0",
    "vitest": "^2.1.5"
  },
  "engines": {
    "node": "22.18.0",
    "npm": "11.5.2"
  }
}
