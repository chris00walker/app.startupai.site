{
  "name": "frontend",
  "version": "1.0.0",
  "main": "postcss.config.js",
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "dev": "next dev --turbopack -p 3001",
    "dev:local": "next dev --turbopack -p 3001",
    "dev:staging": "cd .. && netlify dev",
    "build": "next build",
    "build:staging": "NODE_ENV=staging next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "db:migrate": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:seed": "tsx src/db/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:netlify": "netlify dev --port 8888 & sleep 5 && PRODUCTION_URL=http://localhost:8888 jest src/__tests__/production/ && pkill -f 'netlify dev'",
    "test:function": "netlify functions:invoke onboarding-start --payload '{\"userId\":\"test\",\"planType\":\"trial\"}'",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns='integration'",
    "test:integration": "jest --testPathPatterns='integration'",
    "test:e2e": "pnpm run test:e2e:preflight && pnpm run test:e2e:full",
    "test:e2e:preflight": "tsx scripts/e2e/preflight.ts",
    "test:e2e:smoke": "playwright test tests/e2e/00-smoke.spec.ts tests/e2e/43-hitl-checkpoint-contract.spec.ts",
    "test:e2e:full": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:backend": "playwright test backend-integration.spec.ts",
    "test:e2e:backend:ui": "playwright test backend-integration.spec.ts --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:ci": "pnpm run test:unit && pnpm run test:integration",
    "test:all": "pnpm run test:ci && pnpm run test:e2e",
    "type-check": "tsc --noEmit",
    "lint": "eslint --config eslint.config.warn.mjs src/",
    "lint:baseline": "eslint --config eslint.config.warn.mjs src/ --format json --output-file ../docs/status/linting-baseline.json",
    "lint:strict": "eslint --config eslint.config.base.mjs src/",
    "lint:a11y": "eslint --config eslint.config.a11y.mjs src/",
    "test:generate-token": "tsx scripts/generate-test-token.ts",
    "env:check": "node -e \"console.log('Environment:', process.env.NODE_ENV || 'development'); console.log('Marketing URL:', process.env.NEXT_PUBLIC_MARKETING_URL); console.log('App URL:', process.env.NEXT_PUBLIC_APP_URL); console.log('Site URL:', process.env.NEXT_PUBLIC_SITE_URL);\"",
    "quality-gate": "pnpm -C .. quality-gate",
    "schema:fk": "pnpm -C .. schema:fk",
    "schema:fk:ci": "pnpm -C .. schema:fk:ci",
    "schema:expected": "pnpm exec tsx ../scripts/schema-drift/analyze.ts",
    "schema:test": "pnpm exec tsx ../scripts/schema-drift/__tests__/run.ts",
    "traceability:validate": "pnpm -C .. traceability:validate",
    "figma:sync-tokens": "tsx scripts/figma-sync-tokens.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
