{
  "name": "app-startupai-site",
  "version": "2.0.0",
  "description": "Product Platform for StartupAI Two-Site Architecture",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "scripts": {
    "dev": "pnpm --filter frontend run dev",
    "build": "pnpm --filter frontend run build",
    "start": "pnpm --filter frontend run start",
    "crew:contract-check": "pnpm exec tsx scripts/crew-contract-check.ts",
    "type-check": "pnpm --filter frontend run type-check",
    "test": "pnpm --filter frontend run test",
    "test:e2e": "pnpm --filter frontend run test:e2e",
    "lint": "pnpm --filter frontend run lint",
    "lint:md": "markdownlint docs",
    "lint:md:fix": "markdownlint docs --fix",
    "docs:validate": "pnpm run lint:md",
    "quality-gate": "pnpm type-check && pnpm test && pnpm build && pnpm schema:fk:ci && pnpm traceability:validate",
    "db:generate": "pnpm exec drizzle-kit generate --config=drizzle.config.ts",
    "db:push": "pnpm exec drizzle-kit push --config=drizzle.config.ts",
    "db:migrate": "pnpm exec drizzle-kit migrate --config=drizzle.config.ts",
    "db:studio": "pnpm exec drizzle-kit studio --config=drizzle.config.ts",
    "db:introspect": "pnpm exec drizzle-kit introspect --config=drizzle.config.ts",
    "traceability:generate": "pnpm exec tsx scripts/traceability/generate-story-map.ts",
    "traceability:validate": "pnpm exec tsx scripts/traceability/validate.ts",
    "traceability:gaps": "pnpm exec tsx scripts/traceability/generate-story-map.ts --report gaps",
    "traceability:test": "pnpm exec tsx scripts/traceability/__tests__/run.ts",
    "schema:coverage": "pnpm exec tsx scripts/schema-coverage/analyze.ts",
    "schema:coverage:test": "pnpm exec tsx scripts/schema-coverage/__tests__/run.ts",
    "schema:coverage:ci": "pnpm exec tsx scripts/schema-coverage/analyze.ts --ci",
    "schema:fk": "pnpm exec tsx scripts/schema-coverage/check-fk.ts",
    "schema:fk:ci": "pnpm exec tsx scripts/schema-coverage/check-fk.ts --ci",
    "schema:expected": "pnpm exec tsx scripts/schema-drift/analyze.ts",
    "schema:test": "pnpm exec tsx scripts/schema-drift/__tests__/run.ts"
  },
  "keywords": [
    "nextjs",
    "supabase",
    "crewai",
    "business-validation",
    "two-site-architecture"
  ],
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "24.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "js-yaml": "^4.1.0",
    "markdownlint-cli": "^0.39.0",
    "supabase": "^2.58.5",
    "tsx": "^4.20.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
