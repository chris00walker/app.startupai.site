    "react-to-print": "^3.2.0",
    "recharts": "^3.5.1",
    "resend": "^6.8.0",
    "satori": "^0.19.1",
    "semver": "^7.7.3",
    "sonner": "^2.0.7",
    "stripe": "^20.2.0",
    "tailwind-merge": "^3.3.1",
